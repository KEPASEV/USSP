<script id="maskTemplate" type="text/template">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3> Определение маски</h3>
            <div class="form-group">
                <label for="maskName">Название маски</label>
                <input type="text" id="maskName" class="form-control" placeholder="Введите название маски">
                <label for="maskComment">Комментарий</label>
                <input type="text" id="maskComment" class="form-control" placeholder="Место для маленького комментария">
            </div>
            <div id="maskToolBox" class="row">
                <div class="col-md-12">
                    <div class="btn-group" data-toggle="pill">
                        <button type="button" id="pen" class="btn btn-default"><span class="glyphicon glyphicon-edit"></span></button>
                        <button type="button" id="eraser" class="btn btn-default"><span class="glyphicon glyphicon-unchecked"></span></button>
                        <button type="button" id="directory" class="btn btn-default"><span class="glyphicon glyphicon-collapse-down"></span></button>
                    </div>
                </div>
            </div>

        </div>
        <div class="panel-body">
            <div id="dataTableForMask" class="panel panel-body">
                <div>
                    <table class="table table-bordered">
                        <thead>
                            {{#parameters}}
                            <tr><th>{{name}}</th></tr>
                            {{/parameters}}
                            <tr><th>Отсчеты</th></tr>
                            <tr><th>Справочник</th></tr>
                            {{#variables}}
                            <tr data-var={{id}}><th>{{name}}</th></tr>
                            {{/variables}}
                        </thead>
                    </table>
                </div>
                <div>
                    <table class="table table-bordered">
                        <tbody>
                            {{#parameters}}
                            <tr>
                                {{#value}}
                                <td>{{.}}</td>
                                {{/value}}
                            </tr>
                            {{/parameters}}
                            <tr>
                                {{#ticks}}
                                <td>{{.}}</td>
                                {{/ticks}}
                            </tr>
                            <tr class="shifts">
                                {{#directories}}
                                <td>{{&.}}</td>
                                {{/directories}}
                            </tr>
                            {{#variables}}
                            <tr data-var={{id}} class="values">
                                {{#value}}
                                <td>{{.}}</td>
                                {{/value}}
                            </tr>
                            {{/variables}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="panel-footer">
            <button class="btn btn-success btn-block">Сохранить</button>
        </div>
    </div>
</script>
