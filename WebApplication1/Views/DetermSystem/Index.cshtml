<div class="container">
    <!--NAVIGATION PANEL-->
    <nav class="navbar navbar-default" role="navigation">
        <div class="navbar-header">
            @Html.ActionLink("Универсальный решатель системных задач", "", "Home", null, new { @class = "navbar-brand" })
        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li>
                    @Html.ActionLink("О проекте", "", "About")
                </li>
                <li>
                    @Html.ActionLink("Контакты", "", "Contacts")
                </li>
            </ul>
            <ul class="nav navbar-nav pull-right">
                <li id="user">                   
                </li>
                <li>
                   <a href="#" id="logout">Выход</a>
                </li>               
            </ul>
        </div>
    </nav>
    <!--BODY CONTENT-->
    <!--WIZARD HEAD-->
    <div id="myWizard" class="wizard">
        <ul class="steps">
            <li data-target="#step1" class="active">
                <span class="badge badge-info">1</span>Шаг 1<span class="chevron"></span>
            </li>
            <li data-target="#step2">
                <span class="badge">2</span>Шаг 2<span class="chevron"></span>
            </li>
            <li data-target="#step3">
                <span class="badge">3</span>Шаг 3<span class="chevron"></span>
            </li>
            <li data-target="#step4">
                <span class="badge">4</span>Шаг 4<span class="chevron"></span>
            </li>
            <li data-target="#step5">
                <span class="badge">5</span>Шаг 5<span class="chevron"></span>
            </li>
        </ul>
    </div>
    <!--WIZARD CONTENT-->
    <div class="step-content">
        <div class="step-pane active" id="step1">
            <h2>Шаг 1 - Создание системы</h2>
            <div id="removeModalModule"></div>
            <div class="row">
                <div class="col-md-8">
                    <div id="addSystemModule"></div>
                    <div id="infoSystemModule"></div>
                </div>
                <div class="col-md-4">
                    <div id="listSystemModule"></div>
                </div>
            </div>
        </div>
        <div class="step-pane" id="step2">
            <h2>Шаг 2 - Инициализация системы</h2>
            <div id="addElementModule"></div>
            <div id="listElementModule"></div>
        </div>
        <div class="step-pane" id="step3">
            <h2>Шаг 3 - Загрузка данных</h2>
            <div>
                <form id="fileupload" action="//jquery-file-upload.appspot.com/" method="POST" enctype="multipart/form-data">
                    <!-- The fileupload-buttonbar contains buttons to add/delete files and start/cancel the upload -->
                    <div class="row fileupload-buttonbar">
                        <div class="col-md-7">
                            <span class="btn btn-success fileinput-button">
                                <i class="glyphicon glyphicon-plus"></i>
                                <span>Добавить файлы...</span>
                                <input type="file" name="files[]" multiple>
                            </span>
                            <button type="submit" class="btn btn-primary start">
                                <i class="glyphicon glyphicon-upload"></i>
                                <span>Начать загрузку</span>
                            </button>
                            <button type="reset" class="btn btn-warning cancel">
                                <i class="glyphicon glyphicon-ban-circle"></i>
                                <span>Отменить загрузку</span>
                            </button>
                            <button type="button" class="btn btn-danger delete">
                                <i class="glyphicon glyphicon-trash"></i>
                                <span>Удалить</span>
                            </button>
                            <!-- The global file processing state -->
                            <span class="fileupload-process"></span>
                        </div>
                        <!-- The global progress state -->
                        <div class="col-md-5 fileupload-progress fade">
                            <!-- The global progress bar -->
                            <div class="progress progress-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100">
                                <div class="progress-bar progress-bar-success" style="width:0%;"></div>
                            </div>
                            <!-- The extended global progress state -->
                            <div class="progress-extended">&nbsp;</div>
                        </div>
                    </div>
                    <!-- The table listing the files available for upload/download -->
                    <table role="presentation" class="table table-striped">
                        <tbody class="files"></tbody>
                    </table>
                </form>
                <button type="button" class="btn-lg btn-next" value="next">Дальше</button>
            </div>
        </div>
        <div id="step4" class="step-pane">
            <h2>Шаг 4 - Определение функции абстрагирования</h2>
            <div id="dataGroupModule"></div>
            <div id="addGroupModule"></div>
        </div>
        <div class="step-pane" id="step5">
            <h2>Шаг 5 - Определение системы с поведением</h2>
            <div id="maskModule" class="row"></div>
            <!--<div id="listMaskModule"></div>-->
        </div>
    </div>
    <!--FOOTER-->
    <div class="navbar-fixed-bottom row-fluid">
        <div class="container">
            <p class="text-muted">Универсальный решатель системных задач. Авторы Колесников Антон, Кузнецов Евгений. @DateTime.Now.Year</p>
        </div>
    </div>
</div>
<script data-main="../Scripts/app" src="../Scripts/require.js"></script>
